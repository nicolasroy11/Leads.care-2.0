{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Docker: Attach to Node",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "address": "localhost",
            "localRoot": "${workspaceFolder}/Server/",
            "remoteRoot": "/usr/src/app",
            "protocol": "inspector"
        },
        {
            "name": "Attach to Server Main.js",
            "type": "node",
            "request": "attach",
            "port": 12345,
            "restart": true,
            "protocol": "inspector"
        }
    ]
}