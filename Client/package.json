{
  "name": "leads-client",
  "version": "0.1.1",
  "description": "Client for leads.care",
  "license": "UNLICENSED",
  "main": "Main.js",
  "scripts": {
    "ng": "ng",
    "docker": "docker",
    "dev-start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve-dist": "node Main",
    "pre-docker-build": "ng build --prod",
    "docker-build": "npm run pre-docker-build && docker build -t leads_care:client_$(npm get semver) .",
    "docker-run": "docker run -d --name client-container -p 4200:4200 leads_care:client_$(npm get semver)",
    "docker-br": "npm run docker-build && npm run docker-run",
    "docker-tag-for-deploy": "docker tag leads_care:client_$(npm get semver) $(npm get ecr_uri)/leads_care:client_latest",
    "docker-push-aws": "docker push $(npm get ecr_uri)/leads_care:client_latest",
    "docker-deploy-aws": "npm run docker-build  && npm run docker-tag-for-deploy && npm run docker-push-aws",
    "aws": "aws",
    "aws-test": "aws --version",
    "sh-test": "./scripts/aws_deploy.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.1.3",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^5.0.0-rc0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "mongoose": "^4.11.1",
    "mydatepicker": "^2.0.22",
    "rxjs": "^5.5.2",
    "spin.js": "^2.3.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.5",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "node-sass": "^4.0.0",
    "codelyzer": "~3.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.6.2"
  }
}
